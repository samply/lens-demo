{
  "name": "lens-demo",
  "version": "0.0.1",
  "license": "SEE LICENSE IN LICENSE.md",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "format": "prettier --write \"**/*.{js,ts,d.ts}\"",
    "format:check": "prettier --check \"**/*.{js,ts,d.ts}\"",
    "security:check": "npm audit --production --audit-level high",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:headless": "ng test --browsers=ChromeHeadless --watch=false --code-coverage",
    "analyze": "ng build --configuration production --stats-json && webpack-bundle-analyzer ./dist/lens-demo/stats.json",
    "lint": "ng lint --fix",
    "lint:check": "ng lint",
    "lint:check-commits": "commitlint --from=origin/develop",
    "prepare": "husky install"
  },
  "dependencies": {
    "@angular/animations": "^15.1.0",
    "@angular/common": "^15.1.0",
    "@angular/compiler": "^15.1.0",
    "@angular/core": "^15.1.0",
    "@angular/forms": "^15.2.9",
    "@angular/platform-browser": "^15.1.0",
    "@angular/platform-browser-dynamic": "^15.2.9",
    "@angular/router": "^15.2.9",
    "@samply/lens-components": "^0.0.5",
    "@samply/lens-core": "^0.0.5",
    "chart.js": "^3.9.1",
    "primeflex": "^3.3.1",
    "primeicons": "^6.0.1",
    "primeng": "^15.4.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.5.3",
    "uuid": "^9.0.0",
    "zone.js": "^0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.8",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "~15.1.4",
    "@angular/compiler-cli": "^15.2.9",
    "@angular/localize": "^15.2.9",
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@types/jasmine": "~4.3.0",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "5.48.2",
    "@typescript-eslint/parser": "5.48.2",
    "eslint": "^8.33.0",
    "husky": "^8.0.3",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "prettier": "^2.8.8",
    "typescript": "~4.9.4",
    "webpack-bundle-analyzer": "^4.9.0"
  }
}
